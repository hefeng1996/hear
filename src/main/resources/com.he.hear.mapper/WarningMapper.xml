<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.he.hear.mapper.WarningMapper">
  <resultMap id="BaseResultMap" type="com.he.hear.pojo.Warning">
    <result column="id" jdbcType="INTEGER" property="id" />
    <result column="warn" jdbcType="VARCHAR" property="warn" />
  </resultMap>
  <insert id="insert" parameterType="com.he.hear.pojo.Warning">
    insert into warning (id, warn)
    values (#{id,jdbcType=INTEGER}, #{warn,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="com.he.hear.pojo.Warning">
    insert into warning
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="id != null">
        id,
      </if>
      <if test="warn != null">
        warn,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="id != null">
        #{id,jdbcType=INTEGER},
      </if>
      <if test="warn != null">
        #{warn,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
</mapper>